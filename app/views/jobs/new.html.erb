<div>
  <h1 class="font-bold text-4xl">Jobs#new</h1>
  <p>Find me in app/views/jobs/new.html.erb</p>
</div>

<%= simple_form_for @job do |f| %>
  <%= f.input :title %>
  <%= f.input :project_name %>
  <%= f.input :twitter_link %>
  <%= f.input :os_link %>
  <%= f.input :discord_invite %>
  <%= f.input :description %>
  <%= f.input :chain %>
  <%= f.button :submit %>
<% end %>
